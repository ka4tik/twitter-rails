<% if feed_items.any? %>
    <div class="row">
      <div class="col-md-12">
      <%= will_paginate(feed_items) %>
      <% feed_items.each do |feed_item| %>
      <div class="row">
        <div class="col-md-12">
          <span>
            <%= gravator_for feed_item.user %>
            <%= link_to feed_item.user.name, feed_item.user %>
          </span>
          <span><%= feed_item.content %></span>
          <% if feed_item.image_url%>
          <%= image_tag feed_item.image_url, :class => 'feed-image'%>
          <% end %>
          <span>
            Posted <%= time_ago_in_words(feed_item.created_at) %>
          </span>
        </div>
      </div>
      <% end %>
    <%= will_paginate(feed_items) %>
    </div>
    </div>
<% end %>